{
    "manifest_version": 2,

    "name": "DevToolkit(开发者辅助工具箱)",
    "description": "This extension is a auxiliary toolkit for developers.",
    "version": "1.0",

    "background": {
        "scripts": [
            "assets/jquery/jquery-1.11.0.min.js",
            "resources/js/background.js"
        ]
    },

    "browser_action": {
        "default_title": "DevToolkit",
        "default_icon":"resources/img/icon.png"
    },
    "icons": {
        "128":"resources/img/icon_128.png",
        "48":"resources/img/icon_48.png",
        "16":"resources/img/icon_16.png"
    },

    "permissions": [
        "http://*/"
    ]
}
